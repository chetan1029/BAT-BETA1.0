server {

    server_name beta.thebatonline.com www.beta.thebatonline.com;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /home/batman/projectbat/bat;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/run/gunicorn.sock;
    }

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/thebatonline.com/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/thebatonline.com/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot


}
server {
    if ($host = www.beta.thebatonline.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = beta.thebatonline.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    listen 80;

    server_name beta.thebatonline.com www.beta.thebatonline.com;
    return 404; # managed by Certbot




}
